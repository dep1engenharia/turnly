{
  "name": "@turnly/realtime",
  "version": "0.1.3",
  "description": "Realtime exports the libs, snippets, events, or whatever is useful for any of our services.",
  "homepage": "https://github.com/turnly/turnly#readme",
  "bugs": {
    "url": "https://github.com/turnly/turnly/issues"
  },
  "repository": "https://github.com/turnly/turnly",
  "license": "BSD-3-Clause",
  "author": "Turnly Team <engineering@turnly.app> (https://turnly.app)",
  "main": "dist/index",
  "types": "dist/index",
  "directories": {
    "test": "tests",
    "src": "src"
  },
  "files": [
    "dist",
    "LICENSE",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "prebuild": "rm -rf dist tsconfig.tsbuildinfo",
    "build": "tsc",
    "lint": "prettier \"./**/*.{ts,js}\" --check && eslint . --ext .js,.ts && tsc --noEmit"
  },
  "prettier": "@turnly/eslint-config/.prettierrc.js",
  "eslintConfig": {
    "extends": "@turnly/eslint-config",
    "ignorePatterns": [
      "node_modules",
      "**/*.md",
      "dist",
      "jest*",
      "coverage"
    ]
  },
  "resolutions": {
    "**/esquery": "1.4.0"
  },
  "dependencies": {
    "socket.io": "^4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@turnly/common": "github:turnly/common.git#latest",
    "@turnly/shared": "github:turnly/shared.git#latest",
    "@turnly/eslint-config": "github:turnly/configs",
    "@types/jest": "^27.0.1",
    "@types/node": "^17.0.29",
    "eslint": "^8.34.0",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "prettier": "^2.3.0",
    "ts-jest": "^28.0.5",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=18.14"
  },
  "publishConfig": {
    "@turnly:registry": "https://npm.pkg.github.com/turnly"
  }
}
