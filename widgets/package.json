{
  "name": "@turnly/widgets",
  "version": "0.0.0",
  "private": true,
  "license": "BSD-3-Clause",
  "author": "Turnly Team <team@turnly.app> (https://turnly.app)",
  "exports": {
    ".": {
      "import": "./public/widget.mjs",
      "require": "./public/widget.umd.js"
    }
  },
  "main": "./public/widget.umd.js",
  "module": "./public/widget.mjs",
  "files": [
    "public"
  ],
  "scripts": {
    "prebuild": "rm -rf dist tsconfig.tsbuildinfo",
    "build": "vite build",
    "build:watch": "vite build --watch",
    "codegen": "graphql-codegen --config codegen.yml",
    "serve": "vite preview",
    "start": "cp develop.config.json widget.config.json && concurrently -n \"GraphQL,Styles,Vite\" \"yarn codegen --watch\" \"yarn build:watch\" \"vite --open\""
  },
  "browserslist": {
    "production": [
      "> 1%",
      "last 2 versions",
      "not dead",
      "not IE > 0",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": "@turnly/eslint-config/.prettierrc.js",
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@sentry/integrations": "^7.8.1",
    "@sentry/react": "^7.8.1",
    "@sentry/tracing": "^7.8.1",
    "@turnly/rtm": "github:turnly/realtime-client#latest",
    "clsx": "^1.2.1",
    "framer-motion": "^7.10.2",
    "graphql": "^16.6.0",
    "i18next": "^21.8.16",
    "i18next-browser-languagedetector": "^6.1.4",
    "immer": "^9.0.15",
    "js-cookie": "^3.0.1",
    "preact": "^10.10.0",
    "react-geolocated": "^4.0.3",
    "react-hook-form": "^7.37.0",
    "react-i18next": "^11.18.3",
    "react-icons": "^4.4.0",
    "react-number-format": "^5.0.1",
    "react-phone-input-2": "^2.15.1",
    "react-toastify": "^9.0.8",
    "zustand": "^4.1.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.12.1",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@graphql-codegen/typescript-react-apollo": "3.3.3",
    "@preact/preset-vite": "^2.3.0",
    "@turnly/eslint-config": "github:turnly/configs",
    "@types/js-cookie": "^3.0.2",
    "@types/node": "^18.6.3",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "concurrently": "^7.4.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-tailwindcss": "^3.6.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "sass": "^1.54.0",
    "stylelint": "^14.9.1",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.4"
  },
  "engines": {
    "node": ">=16.15.0"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat",
    "react/jsx-runtime": "preact/jsx-runtime"
  }
}
