{
  "name": "@turnly/business-management",
  "version": "0.1.22",
  "private": true,
  "description": "A service to manage business owners (organizations)",
  "license": "BSD-3-Clause",
  "author": "Turnly Team <team@turnly.app> (https://turnly.app)",
  "scripts": {
    "prebuild": "rm -rf dist tsconfig.tsbuildinfo",
    "build": "tsc -p tsconfig.build.json",
    "watch": "nodemon --config node_modules/@turnly/eslint-config/nodemon.json src/main.ts",
    "lint": "prettier \"./**/*.{ts,js}\" --check && eslint . --ext .js,.ts && tsc --noEmit",
    "test:benchmark": "cross-env NODE_ENV=test echo 'Running benchmark tests... (not implemented yet)'",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:e2e": "cross-env NODE_ENV=test echo 'Running e2e tests... (not implemented yet)'",
    "test:integration": "cross-env NODE_ENV=test jest integration --runInBand",
    "test:unit": "cross-env NODE_ENV=test jest unit"
  },
  "prettier": "@turnly/eslint-config/.prettierrc.js",
  "eslintConfig": {
    "extends": "@turnly/eslint-config",
    "ignorePatterns": [
      "node_modules",
      "**/*.md",
      "dist",
      "jest*",
      "coverage"
    ]
  },
  "dependencies": {
    "@turnly/auth": "^0.1.19",
    "@turnly/common": "^0.1.19",
    "@turnly/core": "^0.1.21",
    "@turnly/grpc": "^0.1.21",
    "@turnly/observability": "^0.1.19",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.1",
    "mongoose": "^7.1.0"
  },
  "devDependencies": {
    "@turnly/eslint-config": "github:turnly/configs",
    "@turnly/testing": "^0.1.21",
    "@types/jest": "^28.1.4",
    "@types/node": "^16.10.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.14.0",
    "jest": "^28.1.1",
    "nodemon": "^2.0.14",
    "prettier": "^2.3.0",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  "resolutions": {
    "**/esquery": "1.4.0"
  },
  "engines": {
    "node": ">=18.14"
  }
}
